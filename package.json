{
  "name": "react-ssr",
  "version": "0.0.1",
  "description": "A middle-man compromise between prerender and SSR.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --coverage && codecov",
    "precompile": "rimraf lib",
    "compile": "babel src --out-dir lib",
    "build": "rollup --config config/rollup.client.config.js",
    "lint": "standard"
  },
  "keywords": [
    "react",
    "react",
    "server",
    "render",
    "react",
    "server-side",
    "rendering"
  ],
  "author": "Owen Ayres",
  "repository": {
    "type": "git",
    "url": "https://github.com/oayres/react-ssr"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^4.1.1",
    "hoist-non-react-statics": "^3.0.1",
    "q": "^1.5.1",
    "regenerator": "^0.14.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "jest": "^23.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "3.0.7",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "standard": "^12.0.1"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.0.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.0.0"
  },
  "standard": {
    "globals": [
      "expect",
      "test",
      "describe",
      "it",
      "beforeAll",
      "shallow",
      "mount",
      "snapshot"
    ],
    "parser": "babel-eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "testURL": "http://localhost/",
    "setupFiles": [
      "./config/jest.config.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/lib",
      "<rootDir>/examples",
      "<rootDir>/config"
    ],
    "verbose": true,
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/index.js",
      "!src/fetchData.js"
    ]
  }
}
